{
  "name": "nodejs-refresh-token",
  "version": "1.1.11",
  "description": "Node.js lib for authentication",
  "types": "lib/index.d.ts",
  "main": "lib/index.js",
  "jest": {
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "testRegex": "\\.spec\\.ts",
    "moduleFileExtensions": [
      "ts",
